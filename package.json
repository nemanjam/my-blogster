{
  "name": "my-blogster",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "build:blog": "turbo run blog#build",
    "manypkg:check": "manypkg check",
    "manypkg:fix": "manypkg fix",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "turbo run format"
  },
  "devDependencies": {
    "config-eslint": "*",
    "config-typescript": "*",
    "config-prettier": "*",
    "@manypkg/cli": "^0.21.1",
    "turbo": "^1.11.2"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.9.0"
  }
}
