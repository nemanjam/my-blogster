{
  "name": "blogster",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/config/*",
    "packages/*",
    "blog"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "build:blog": "turbo run @blog#build",
    "manypkg:check": "manypkg check",
    "manypkg:fix": "manypkg fix",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@manypkg/cli": "^0.21.1",
    "eslint-config-custom": "*",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "yarn@1.22.19"
}
