{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "blog#build": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {},
    "check-types": {}
  }
}
